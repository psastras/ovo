{
  "name": "@psastras/ovo",
  "bin": {
    "ovo": "./index.js"
  },
  "version": "0.0.0-development",
  "description": "An alternate Zipkin UI",
  "keywords": [
    "zipkin",
    "zipkinui",
    "zipkin-ui"
  ],
  "main": "src/index.tsx",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js --progress",
    "build:test": "rimraf .build && webpack --config webpack.config.test.js",
    "build:server": "webpack --config server/webpack.config.js --progress",
    "install:selenium": "selenium-standalone install",
    "pretest": "npm-run-all build:test",
    "test": "nyc ava --verbose",
    "precitest": "npm-run-all build:test",
    "citest": "nyc ava --tap | tap-xunit > $CI_TEST_REPORT_FILE",
    "postcitest": "npm-run-all lint && nyc report --reporter=json && codecov -f coverage/*.json",
    "lint": "tslint --project src/tsconfig.json",
    "preintegrationtest": "webpack --config webpack.config.test.js",
    "integrationtest:test": "ava '.build/integration-test/**/*.js' --verbose -c 1",
    "integrationtest:runner": "node .build/integration-test/_runner.ts",
    "integrationtest": "npm-run-all build integrationtest:runner",
    "server": "node dist/server.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:server": "webpack-dev-server",
    "watch:test": "webpack --config webpack.config.test.js --watch",
    "watch:ava": "ava --watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "ava": {
    "files": [
      ".build/test/**/*.js"
    ],
    "require": [
      "jsdom-global/register",
      "source-map-support/register"
    ],
    "concurrency": 4
  },
  "nyc": {
    "exclude": [
      ".build"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^2.5.39",
    "@types/express": "^4.0.34",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.52",
    "@types/nprogress": "^0.0.29",
    "@types/react": "^0.14.50",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.34",
    "@types/react-router": "^2.0.39",
    "@types/react-router-redux": "^4.0.34",
    "@types/redux-actions": "^1.2.0",
    "@types/redux-logger": "^2.6.32",
    "@types/redux-promise": "^0.5.23",
    "@types/superagent": "^2.0.36",
    "@types/webdriverio": "^4.4.0",
    "antd": "^2.5.2",
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "codecov": "^1.0.1",
    "condition-circle": "^1.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.24.1",
    "inject-loader": "^2.0.0",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "json-format": "^1.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "node-sass": "^4.0.0",
    "npm-run-all": "^3.1.1",
    "nprogress": "^0.2.0",
    "nyc": "^10.0.0",
    "rc-animate": "^2.3.1",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-test-utils": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-loader": "^2.4.0",
    "react-parallax": "^1.1.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.0",
    "selenium-standalone": "^5.8.0",
    "semantic-release": "^6.3.2",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.6",
    "style-loader": "^0.13.1",
    "superagent": "^3.3.1",
    "tap-xunit": "^1.5.0",
    "ts-loader": "^1.3.3",
    "tslint": "^4.1.1",
    "typescript": "^2.1.4",
    "webdriverio": "^4.5.0",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-node-externals": "^1.5.4",
    "zipkin": "^0.5.0",
    "zipkin-context-cls": "^0.2.1",
    "zipkin-instrumentation-express": "^0.5.0",
    "zipkin-transport-http": "^0.5.0"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "express": "^4.19.2",
    "request": "^2.79.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psastras/ovo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psastras/ovo/issues"
  },
  "homepage": "https://github.com/psastras/ovo#readme"
}
